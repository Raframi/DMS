@model IEnumerable<DMSModels.Keyword>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.KeywordName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.KeywordName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.KeywordId }) |
            @Html.ActionLink("Details", "Details", new { id=item.KeywordId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.KeywordId })
        </td>
    </tr>
}

</table>
